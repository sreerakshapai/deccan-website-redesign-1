extends partials/head.pug

block title 
        title Ritz Multi Seating System

block content
    .container
        .card
            .card-image
                img(src="images/categories/study_chair.png", class="category-page-img", alt=" Ritz Multi Seating System, Deccan Plast Industries")
            .card-content
                h2.category-title.center-align Ritz(Shell) Multi Seating System
                p.category-description 
                    strong  Ritz 
                    | Mass seating system are versatile and modular with multiple applications ike single seater, office models, conference chairs for offices, multiseater for waiting areas, student study chair models for schools, collages, coaching classes, libraries etc.
                p.category-description
                strong Dimensions :
                | Single Seat: (H) 375 mm, (W) 425 mm, (D) 430 mm
  
                p.category-description 
                strong Key Features:
                |
                ul
                    li.feature-list  Versatile Modular Seating
                    li.feature-list Multi-purpose Applications
                    li.feature-list  Available in attractive colors
                .card.product
                    .card-content
                        .row.center-align
                            .col.m6.l3
                                .card.product-card.ritz-card.z-depth-2
                                    .card-image
                                        img(src="images/products/furniture/s_ritz_1.png", alt="Ritz Multi Seating System, Deccan Plast Industries.")
                                    .card-content.center-align
                                        span.card-title.grey-text.text-darken-4 Visitor
                                        p.price MRP: ₹ 425/-
                                        .right-align
                                             a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Visitor")
                                                i.material-icons shopping_cart  
                            .col.m6.l3
                                .card.product-card.ritz-card.z-depth-2
                                    .card-image
                                        img(src="images/products/furniture/s_ritz_2.png", alt="Ritz Multi Seating System, Deccan Plast Industries.")
                                    .card-content.center-align
                                        span.card-title.grey-text.text-darken-4 Conference 
                                        p.price MRP: ₹ 425/-
                                        .right-align
                                        span.card-title.grey-text.text-darken-4  
                                             a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Conference")
                                                i.material-icons shopping_cart  
                            .col.m6.l3
                                .card.product-card.ritz-card.z-depth-2
                                    .card-image
                                        img(src="images/products/furniture/s_ritz_3.png", alt="Ritz Multi Seating System, Deccan Plast Industries.")
                                    .card-content.center-align
                                        span.card-title.grey-text.text-darken-4 Study Chair 
                                        p.price MRP: ₹ 425/-
                                        .right-align
                                            a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Study Chair")
                                                i.material-icons shopping_cart   
                            .col.m6.l3
                                .card.product-card.ritz-card.z-depth-2
                                    .card-image
                                        img(src="images/products/furniture/s_ritz_4.png", alt="Ritz Multi Seating System, Deccan Plast Industries.")
                                    .card-content.center-align
                                        span.card-title.grey-text.text-darken-4 Study Chair(Rack)
                                        p.price MRP: ₹ 425/-
                                        .right-align
                                             a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Study Chair(Rack)")
                                                i.material-icons shopping_cart          
                            .col.m6.l3
                                .card.product-card.ritz-card.z-depth-2
                                    .card-image
                                        img(src="images/products/furniture/s_ritz_5.png", alt="Ritz Multi Seating System, Deccan Plast Industries.")
                                    .card-content.center-align
                                        span.card-title.grey-text.text-darken-4 Revolving Chair with Wheels
                                        p.price MRP: ₹ 425/-
                                        .right-align
                                             a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item=" Revolving Chair with Wheels")
                                                i.material-icons shopping_cart                
                .center-align                                                                    
                    a(href="furniture.html" class="waves-effect waves-light btn-large red") 
                        i.material-icons.left home
                        | Home
                #quote-modal.modal
                    .modal-content
                        h3 Modal Header
                        p Please fill all your details.
                        form(action="", id="request-quote-form")
                            .input-field
                                input(placeholder="John Doe", id="rq-name", type="text", class="validate")
                                label(for="rq-name") Name
                            .input-field
                                input(placeholder="johndoe@gmail.com", id="rq-email", type="email", class="validate")
                                label(for="rq-email") Email
                            .input-field
                                input(placeholder="10 Digit Mobile No", id="rq-phone", type="tel", class="validate")
                                label(for="rq-phone") Phone    
                            .input-field
                                input(placeholder="575001", id="rq-pincode", type="number", class="validate")
                                label(for="rq-pincode") Pincode
                            .input-field
                                input(placeholder="Mangalore", id="rq-location", type="text", class="validate")
                                label(for="rq-location") Location
                            .input-field
                                textarea#rq-message.materialize-textarea  
                                label(for="rq-message")  Message                                                                                           
                    .modal-footer
                        a.modal-send.waves-effect.waves-green.btn.btn-flat(href="#") Send
                        a.modal-close.waves-effect.waves-green.btn.btn-flat(href="#") Close



