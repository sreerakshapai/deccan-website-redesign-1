                                             a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Mangalore DBR")
                                                i.material-icons shopping_cart                          
                .card.product
                    .card-content
                        h3.product-title.center-align Calicut Round Dining Tables
                        .row.center-align
                            .col.m6.l3.offset-l1
                                .card.product-card.z-depth-2
                                    .card-image
                                        img(src="images/products/furniture/s_calicut_round_3.png", alt="Calicut Dining Tables, Deccan Plast Industries.")
                                    .card-content.center-align
                                        span.card-title.grey-text.text-darken-4 Calicut RED
