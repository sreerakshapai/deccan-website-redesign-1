extends partials/head.pug

block title 
        title Dining Tables

block content
    .container
        .card
            .card-image
                img(src="images/categories/dining_tables.png", class="category-page-img", alt="Dining Tables, Deccan Plast Industries")
            .card-content
                h2.category-title.center-align Dining Tables
                p.category-description Our Dining tables are a must for restaurants and road side fast food joints and dhabas, besides lawns of elite homes.
                p.category-description 
                strong Dimensions: 
                |(H) 1150 mm, (W) 725 mm, (D) 725 mm

                p.category-description
                strong  Key Features:
                |
                ul
                    li.feature-list  Sturdy and Strong
                    li.feature-list Suitable for seating 4 people
                    li.feature-list  Can be packed in a box
                .card.product
                    .card-content
                        h3.product-title.center-align Mangalore Dining Tables

                        .row.center-align
                            .col.m6.l3
                                .card.product-card.z-depth-2
                                    .card-image
                                        img(src="images/products/furniture/s_mangalore_dining_3.png", alt="Mangalore Dining Tables, Deccan Plast Industries.")
                                    .card-content.center-align
                                        span.card-title.grey-text.text-darken-4 Mangalore RED
                                        p.price MRP: ₹ 1770/-
                                        .right-align
                                             a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Mangalore RED")
                                                i.material-icons shopping_cart       
                            .col.m6.l3
                                .card.product-card.z-depth-2
                                    .card-image
                                        img(src="images/products/furniture/s_mangalore_dining_5.png", alt="Mangalore Dining Tables, Deccan Plast Industries.")
                                    .card-content.center-align
                                        span.card-title.grey-text.text-darken-4 Mangalore RWD 
                                        p.price MRP: ₹ 1770/-
                                        .right-align
                                            a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Mangalore RWD")
                                                i.material-icons shopping_cart       
                            .col.m6.l3
                                .card.product-card.z-depth-2
                                    .card-image
                                        img(src="images/products/furniture/s_mangalore_dining_1.png", alt="Mangalore Dining Tables, Deccan Plast Industries.")
                                    .card-content.center-align
                                        span.card-title.grey-text.text-darken-4 Mangalore PWD 
                                        p.price MRP: ₹ 1770/-
                                        .right-align
                                             a.btn-floating.waves-effect.waves-light.red.request-quote.modal-trigger(data-target="quote-modal", data-item="Mangalore PWD")
                                                i.material-icons shopping_cart       
                            .col.m6.l3
                                .card.product-card.z-depth-2
                                    .card-image
                                        img(src="images/products/furniture/s_mangalore_dining_2.png", alt="Mangalore Dining Tables, Deccan Plast Industries.")
                                    .card-content.center-align
